- if @project.user.integrated_to_raiseit?
  = link_to  'Integrado com Raiseit! Clique aqui para redefinir sua senha', @project.user.reset_password_url, class:'btn btn-large raiseit-on', target: '_blank'
-else
  = link_to  'Integrar com Raiseit', api_create_path(user_id: @project.user.id), class:'btn btn-large raiseit-off', id: 'raiseit-save', method: :post
